{
    "name": "epicub/rome2rio",
    "type": "symfony-bundle",
    "description": "Interface for used Rome2Rio system API and cache the responses",
    "keywords": ["Cache","API","Rome2Rio","Symfony2"],
    "homepage": "https://github.com/EPICUB/r2Rio",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Pedro Perez Gonzalez",
            "email": "xeroz.pglez@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.2",
        "ext-curl": "*",
        "leaseweb/api-caller-bundle":"dev-master"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "*"
    },
    "autoload": {
        "psr-0": { "Epicub\\R2RioBundle": "" }
    },
    "target-dir": "Epicub/R2RioBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
